public class FpsModule extends HudModule {

    public FpsModule() {
        super("FPS", Keyboard.KEY_NONE);
    }

    @Override
    public void render() {
        Minecraft mc = Minecraft.getMinecraft();
        mc.fontRendererObj.drawString("FPS: " + Minecraft.getDebugFPS(), 5, 5, 0xFFFFFF);
    }

    @Override
    public void onEnable() {}
    @Override
    public void onDisable() {}
}
